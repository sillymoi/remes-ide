<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2006/GraphicalDefinition" name="uppaallite">
  <figures
      name="Default">
    <descriptors
        name="LocationTypeFigure">
      <actualFigure
          xsi:type="gmfgraph:Ellipse"
          name="LocationTypeFigure">
        <backgroundColor
            xsi:type="gmfgraph:ConstantColor"
            value="orange"/>
      </actualFigure>
    </descriptors>
    <descriptors
        name="TransitionTypeFigure">
      <actualFigure
          xsi:type="gmfgraph:PolylineConnection"
          name="TransitionTypeFigure"
          targetDecoration="//@figures.0/@descriptors.1/@actualFigure/@children.0">
        <children
            xsi:type="gmfgraph:PolylineDecoration"
            name="TransitionTypeArrow">
          <template
              x="-1"
              y="1"/>
          <template/>
          <template
              x="-1"
              y="-1"/>
        </children>
      </actualFigure>
    </descriptors>
    <descriptors
        name="LocationNameFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="LocationNameFigure"/>
    </descriptors>
    <descriptors
        name="LocationInvariantFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="LocationInvariantFigure"/>
    </descriptors>
    <descriptors
        name="TransitionAssignmentFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="TransitionAssignmentFigure"/>
    </descriptors>
    <descriptors
        name="TransitionGuardFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="TransitionGuardFigure"/>
    </descriptors>
    <descriptors
        name="TransitionSyncFigure">
      <actualFigure
          xsi:type="gmfgraph:Label"
          name="TransitionSyncFigure"/>
    </descriptors>
    <descriptors
        name="LocationTypeCommitedFigure">
      <actualFigure
          xsi:type="gmfgraph:SVGFigure"
          name="LocationTypeCommitedFigure"
          documentURI="platform:/plugin/hr.fer.rasip.uppaallite.diagram/icons/LocationTypeCommitedImage.svg"
          noCanvasWidth="true"
          noCanvasHeight="true">
        <maximumSize
            dx="32"
            dy="32"/>
        <minimumSize
            dx="32"
            dy="32"/>
      </actualFigure>
    </descriptors>
    <descriptors
        name="LocationTypeUrgentFigure">
      <actualFigure
          xsi:type="gmfgraph:SVGFigure"
          name="LocationTypeUrgentFigure"
          documentURI="platform:/plugin/hr.fer.rasip.uppaallite.diagram/icons/LocationTypeUrgentImage.svg"
          noCanvasWidth="true"
          noCanvasHeight="true">
        <maximumSize
            dx="32"
            dy="32"/>
      </actualFigure>
    </descriptors>
  </figures>
  <nodes
      name="LocationType"
      figure="LocationTypeFigure"
      resizeConstraint="NONE"/>
  <nodes
      name="LocationTypeCommited"
      figure="LocationTypeCommitedFigure"
      resizeConstraint="NONE"/>
  <nodes
      name="LocationTypeUrgent"
      figure="LocationTypeUrgentFigure"
      resizeConstraint="NONE"/>
  <connections
      name="TransitionType"
      figure="TransitionTypeFigure"/>
  <labels
      name="LocationName"
      figure="LocationNameFigure"/>
  <labels
      name="LocationInvariant"
      figure="LocationInvariantFigure"/>
  <labels
      name="TransitionAssignment"
      figure="TransitionAssignmentFigure"/>
  <labels
      name="TransitionGuard"
      figure="TransitionGuardFigure"/>
  <labels
      name="TransitionSync"
      figure="TransitionSyncFigure"/>
</gmfgraph:Canvas>
