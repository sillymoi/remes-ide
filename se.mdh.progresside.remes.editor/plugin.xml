<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="se.mdh.progresside.remes.presentation.RemesModelWizardID"
            name="%_UI_RemesModelWizard_label"
            class="se.mdh.progresside.remes.presentation.RemesModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RemesModelFile.gif">
         <description>%_UI_RemesModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="se.mdh.progresside.remes.presentation.RemesEditorID"
            name="%_UI_RemesEditor_label"
            icon="icons/full/obj16/RemesModelFile.gif"
            extensions="remes"
            class="se.mdh.progresside.remes.presentation.RemesEditor"
            contributorClass="se.mdh.progresside.remes.presentation.RemesActionBarContributor">
      </editor>
   </extension>

</plugin>
